{
  "semi": false,
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "es5",
  "jsxBracketSameLine": true,
  "importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
  "importOrderSeparation": true,

  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": false,
  "organizeImportsSkipDestructiveCodeActions": true,
  "printWidth": 100
}
